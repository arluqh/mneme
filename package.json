{
  "name": "mneme",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "slidev build slides/sample/slides.md --out dist/sample",
    "build:all": "for d in slides/*; do if [ -f \"$d/slides.md\" ]; then slidev build \"$d/slides.md\" --out dist/$(basename $d); fi; done",
    "dev": "slidev --open",
    "export": "slidev export"
  },
  "dependencies": {
    "@slidev/cli": "^52.8.0",
    "@slidev/theme-default": "latest",
    "@slidev/theme-seriph": "latest",
    "vue": "^3.5.24"
  }
}